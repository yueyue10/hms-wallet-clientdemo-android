<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zyj.testhms">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.huawei.android.launcher.permission.CHANGE_BADGE" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!-- AppId is the value in the developer alliance app infomation -->
        <meta-data
            android:name="com.huawei.hms.client.appid"
            android:value="103685579" />

        <activity android:name=".PageHomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PassDataObjectActivity" />
        <activity android:name=".GiftCardActivity" />
        <activity android:name=".CouponCardActivity" />
        <activity android:name=".PassTestActivity" />
        <activity android:name=".WalletPassCnActivity" />
        <activity android:name=".MainIndexActivity" />
        <activity android:name=".apptest.PushGetTokenActivity" />

        <meta-data
            android:name="push_kit_auto_init_enabled"
            android:value="true" />
        <service
            android:name=".apptest.PushMsgService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.huawei.push.action.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>
</manifest>

