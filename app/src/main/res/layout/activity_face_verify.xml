<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/temp_Preview"
        android:layout_width="match_parent"
        android:layout_height="200dp" />

    <Button
        android:id="@+id/select_picture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="选择模型图片" />

    <Button
        android:id="@+id/create_model"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="创建模型" />

    <TextView
        android:id="@+id/create_model_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/compare_face_image"
        android:layout_width="match_parent"
        android:layout_height="200dp" />

    <Button
        android:id="@+id/select_compare_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="选择对比图片" />

    <Button
        android:id="@+id/compare_face"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="对比人脸" />

    <TextView
        android:id="@+id/compare_face_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>